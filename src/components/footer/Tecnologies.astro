---
import FooterLink from './FooterLink.astro';
import { technologies } from '@/data/data.json';
---

<nav class="flex flex-col gap-2 items-start">
  <h3>Powered by</h3>
  {
    Object.entries(technologies).map(([key, value]) => (
      <FooterLink title={value.title} link={value.url} icon={value.icon} />
    ))
  }
</nav>
